#!/bin/bash
#set -x
# wmctrl -l -p -G
# #Wid desk pid x-off y-off width height host title
LOGdir="/home/jim/tools/Ecobee/logs"
for f in $(ls -tr $LOGdir/E* | tail -3); do
    echo $f | grep -q NC
    if [[ $? -eq 0 ]]; then
	LOC="-e 0,2862,22,968,1384"
    else
	echo $f | grep -q SC
	if [[ $? -eq 0 ]]; then
	    LOC="-e 0,0,22,968,1384"
	else
	    LOC="-e 0,978,22,832,1384"
	fi
    fi
    /usr/bin/emacsclient -c +999999 $f &
    sleep 1
    CWID=$(wmctrl -lp | grep $f | cut -d " " -f 1)
    wmctrl -i -r "$CWID" $LOC 
done
echo " "
